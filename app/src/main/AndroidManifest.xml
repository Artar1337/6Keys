<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.sibsutis.a6keys">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:screenOrientation="landscape"
        android:supportsRtl="true"
        android:theme="@style/Theme.6Keys">
        <activity android:name=".MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ru.sibsutis.a6keys.PrefsActivity"
            android:label="@string/prefs"
            android:noHistory="false" />
        <activity
            android:name="ru.sibsutis.a6keys.MathActivity"
            android:label="@string/solveMath"
            android:noHistory="true" />
        <activity
            android:name="ru.sibsutis.a6keys.GameActivity"
            android:label="@string/app_name" />
        <activity
            android:name="ru.sibsutis.a6keys.CardActivity"
            android:label="@string/solveCard"
            android:noHistory="true" />
        <activity
            android:name="ru.sibsutis.a6keys.LogicActivity"
            android:label="@string/solveLogic"
            android:noHistory="true" />
        <activity
            android:name="ru.sibsutis.a6keys.PicActivity"
            android:label="@string/solveLogic"
            android:noHistory="true" />
        <activity
            android:name="ru.sibsutis.a6keys.EstimateActivity"
            android:label="@string/solveLogic"
            android:noHistory="true" />
        <activity
            android:name="ru.sibsutis.a6keys.QuestionsActivity"
            android:label="@string/solveLogic"
            android:noHistory="true" />
    </application>

</manifest>